<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GraphController">
   <children>
      <BorderPane layoutX="-2.0" layoutY="-3.0" prefHeight="328.0" prefWidth="502.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <HBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <CheckBox fx:id="gridLines" mnemonicParsing="false" onAction="#handleGridLines" text="Show Grid Lines">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox mnemonicParsing="false" onAction="#handleXAxis" text="Show X-Axis" fx:id="xAxis">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="yAxis" mnemonicParsing="false" onAction="#handleYAxis" text="Show Y-Axis">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="centerSelector" layoutX="293.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleCenterSelectors" text="Show Center Dots">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="connections" layoutX="419.0" layoutY="20.0" mnemonicParsing="false" text="Show Connections">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </CheckBox>
                  <Region HBox.hgrow="ALWAYS" />
                  <Label alignment="BOTTOM_RIGHT" text="Coordinates:">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="coordinateLabel" alignment="BOTTOM_RIGHT" text="No tile selected">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </bottom>
         <right>
            <TextArea fx:id="output" editable="false" minWidth="250.0" wrapText="true" BorderPane.alignment="CENTER">
               <font>
                  <Font size="10.0" />
               </font></TextArea>
         </right>
         <center>
            <ScrollPane fitToHeight="true" fitToWidth="true" style="-fx-background-color: #738196;" BorderPane.alignment="CENTER">
               <content>
                  <StackPane fx:id="nodePane" prefWidth="800.0" style="-fx-background-color: #738196;">
                     <children>
                        <Group fx:id="superGroup" StackPane.alignment="CENTER" />
                     </children>
                  </StackPane>
               </content>
            </ScrollPane>
         </center>
         <left>
            <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <ColorPicker fx:id="colorPicker" />
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </left>
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top></BorderPane>
   </children>
</AnchorPane>
